{
    "name": "macpaw/sentry-monolog-adapter",
    "description": "Sentry monolog logging bundle",
    "type": "symfony-bundle",
    "keywords": [
        "MacPaw",
        "symfony",
        "sentry",
        "monolog",
        "logging"
    ],
    "homepage": "https://github.com/Macpaw>",
    "license": "MIT",
    "authors": [
        {
            "name": "Yozhef Hisem",
            "email": "hisemjo@gmail.com",
            "homepage": "https://macpaw.com/",
            "role": "Software Engineer"
        },
        {
            "name": "Ihor Roik",
            "email": "roidloua@gmail.com",
            "homepage": "https://macpaw.com/",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/monolog-bundle": "^3.5",
        "sentry/sentry-symfony": "^4.0",
        "symfony/messenger": "^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "0.12.*",
        "phpunit/phpunit": "9.3.*",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "SentrySymfony\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SentrySymfony\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0",
            "dev-develop": "1.0.x-dev"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.0"
        }
    }
}
